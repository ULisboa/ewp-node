<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
  PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
  "http://www.tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>
  <rule>
    <name>REST URI prefix to API URI prefix</name>
    <from>^/rest/(.*)$</from>
    <to type="forward">/api/$1</to>
  </rule>
</urlrewrite>